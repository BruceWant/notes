# display the symbol tables of each executable
objdump -t <program>


# disassembly executable file to assembly language.
objdump -d <program_name> > prog1.dump

# disassembly executable file to asssembly language in Intel syntax.
objdump -d -M intel <program_name>

# display all architecture
objdump -i
objdump --info


# reverse a binary file to assembly language
objdump -D -bbinary <binary_file> -m<architecutre>

